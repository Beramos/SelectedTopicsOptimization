issn = {1751-7362},
journal = {The ISME Journal},
title = {{A virus or more in (nearly) every cell: ubiquitous networks of virus–host interactions in extreme environments}},
url = {https://www.nature.com/articles/s41396-018-0071-7.pdf http://www.nature.com/articles/s41396-018-0071-7},
year = {2018}
}
