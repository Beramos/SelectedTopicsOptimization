journal = {Bio Systems},
keywords = {Bacterial,Bacterial Physiological Phenomena,Bacterial Proteins,Bacterial Proteins: metabolism,Bacterial: physiology,Biological,Biological Evolution,Cell Communication,Cell Communication: physiology,Cell Proliferation,Computer Simulation,Computing Methodologies,Gene Expression Regulation,Models,Signal Transduction,Signal Transduction: physiology},
