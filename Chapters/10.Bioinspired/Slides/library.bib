journal = {Database : The Journal of Biological Databases and Curation},
pages = {bas028},
pmid = {22730453},
title = {{MuteinDB: the mutein database linking substrates, products and enzymatic reactions directly with genetic variants of enzymes.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22730453 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3381223},
volume = {2012},
year = {2012}
}
