keywords = {Factorization machine,Sparse data,Support vector machine,Tensor factorization},
pages = {995--1000},
pmid = {1530748},
title = {{Factorization machines}},
url = {https://www.csie.ntu.edu.tw/{~}b97053/paper/Rendle2010FM.pdf},
year = {2010}
}
