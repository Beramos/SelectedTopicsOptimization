keywords = {asia,birds,frugivory,mammals,oriental region,seed dispersal,seed predation},
number = {4},
pages = {413--448},
pmid = {9951414},
publisher = {Elsevier Ltd},
title = {{Frugivory and seed dispersal by vertebrates in the Oriental (Indomalayan) Region.}},
url = {http://dx.doi.org/10.1016/j.gecco.2017.04.007},
volume = {73},
year = {1998}
}
