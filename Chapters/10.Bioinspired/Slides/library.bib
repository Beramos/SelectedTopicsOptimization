file = {:Users/michielstock/Documents/Articles/Ecology/Wood{\_}et{\_}al-2015-Ecology{\_}and{\_}Evolution.pdf:pdf},
isbn = {2045-7758},
issn = {20457758},
journal = {Ecology and Evolution},
keywords = {Complexity,Diet,Diversity,Predation,Sampling area,Scale dependence,Trophic interactions,datasets,food webs},
mendeley-tags = {datasets,food webs},
