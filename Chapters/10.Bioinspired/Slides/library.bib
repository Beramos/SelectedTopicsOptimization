pages = {607--628},
pmid = {15032608},
title = {{The catharanthus alkaloids: pharmacognosy and biotechnology}},
url = {http://www.eurekaselect.com/openurl/content.php?genre=article{\&}issn=0929-8673{\&}volume=11{\&}issue=5{\&}spage=607},
