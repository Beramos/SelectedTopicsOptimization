Voorbeeld bos ecosysteem beheer.},
author = {Evans, Darren M. and Kitson, James J N and Lunt, David H. and Straw, Nigel A. and Pocock, Michael J O},
doi = {10.1111/1365-2435.12659},
file = {:Users/michielstock/Documents/Articles/Ecology/interactions/Evans{\_}et{\_}al-2016-Functional{\_}Ecology.pdf:pdf},
isbn = {1365-2435},
issn = {13652435},
journal = {Functional Ecology},
keywords = {Food webs,Forestry,Host-parasitoid interactions,Invasive species,Next-generation sequencing},
number = {12},
