volume = {120},
year = {2010}
}
@article{Shimizu2014,
abstract = {Relationships between flowers and pollinators are generally considered cases of mutualism since both agents gain benefits. Fine-tuned adaptations are usually found in the form of strict one-to-one coevolution between species. Many insect pollinators are, however, considered generalists, visiting numerous kinds of flowers, and many flower species (angiosperms) are also considered generalists, visited by many insect pollinators. We here describe a fine-tuned coevolutionary state of a flower-visiting bee that collects both nectar and pollen from an early spring flower visited by multiple pollinators. Detailed morphology of the bee proboscis is shown to be finely adjusted to the floral morphology and nectar production of the flower. Behavioral observations also confirm the precision of this mutualism. Our results suggest that a fine-tuned one-to-one coevolutionary state between a flower species and a pollinator species might be common, but frequently overlooked, in multiple flower-pollinator interactions.},
author = {Shimizu, Akira and Dohzono, Ikumi and Nakaji, Masayoshi and Roff, Derek A and Miller, Donald G and Osato, Sara and Yajima, Takuya and Niitsu, Sh{\^{u}}hei and Utsugi, Nozomu and Sugawara, Takashi and Yoshimura, Jin},
doi = {10.1038/srep03988},
file = {:Users/michielstock/Library/Application Support/Mendeley Desktop/Downloaded/Shimizu et al. - 2014 - Fine-tuned bee-flower coevolutionary state hidden within multiple pollination interactions.pdf:pdf},
isbn = {0192880519},
issn = {2045-2322},
journal = {Scientific Reports},
keywords = {Adaptation,Animals,Bees,Bees: anatomy {\&} histology,Bees: physiology,Biological Evolution,Flowers,Lonicera,Lonicera: embryology,Phylogeny,Physiological,Plant Nectar,Pollen,Pollination,Pollination: physiology,Symbiosis,coevolution,pollination},
mendeley-tags = {coevolution,pollination},
number = {3988},
pages = {1--9},
pmid = {24496444},
title = {{Fine-tuned bee-flower coevolutionary state hidden within multiple pollination interactions}},
url = {http://www.nature.com/srep/2014/140205/srep03988/full/srep03988.html},
volume = {4},
year = {2014}
}
