keywords = {biotic interactions,climate adaptation,mutualisms,rhizosphere,stress-gradient},
pages = {031195},
title = {{Extending the Stress-Gradient hypothesis: increased local adaptation between teosinte and soil biota at the stressful end of a climate gradient}},
