issn = {1553-7358},
journal = {PLoS computational biology},
keywords = {Amino Acid Sequence,Animals,Computational Biology,Computational Biology: methods,Databases, Protein,Humans,Neural Networks (Computer),Predictive Value of Tests,Protein Interaction Mapping,Protein Interaction Mapping: methods,Proteins,Proteins: chemistry,Proteins: metabolism,Structural Homology, Protein,Structure-Activity Relationship,Systems Integration},
month = {jan},
number = {7},
pages = {e1000118},
pmid = {18654616},
title = {{Predicting and validating protein interactions using network structure.}},
