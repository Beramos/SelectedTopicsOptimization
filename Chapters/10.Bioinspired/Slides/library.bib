keywords = {abstract understanding and predicting,allometry,energy flow,food web,generally require estimates of,interaction strength among species,measuring inter-,modeling,scale,systems,the dynamics of multispecies},
number = {1},
pages = {419--444},
pmid = {234684900018},
title = {{Measurement of interaction strength in nature}},
url = {http://www.annualreviews.org/doi/abs/10.1146/annurev.ecolsys.36.091704.175535},
volume = {36},
