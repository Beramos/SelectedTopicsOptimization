file = {:Users/michielstock/Library/Application Support/Mendeley Desktop/Downloaded/Mei et al. - 2013 - Drug-target interaction prediction by learning from local information and neighbors.pdf:pdf},
isbn = {1367-4811 (Linking)},
issn = {13674803},
journal = {Bioinformatics},
number = {2},
pages = {238--245},
pmid = {23162055},
title = {{Drug-target interaction prediction by learning from local information and neighbors}},
volume = {29},
year = {2013}
}
