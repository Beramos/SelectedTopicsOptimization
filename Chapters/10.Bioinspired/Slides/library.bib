journal = {BMC Proceedings},
keywords = {machine-learning,structured-output},
number = {Suppl 4},
pages = {S2+},
pmid = {19091049},
title = {{Towards structured output prediction of enzyme function}},
url = {http://view.ncbi.nlm.nih.gov/pubmed/19091049},
volume = {2},
year = {2008}
}
