number = {11},
pages = {891--906},
pmid = {20814717},
title = {{Trainable structure–activity relationship model for virtual screening of CYP3A4 inhibition}},
url = {http://link.springer.com/10.1007/s10822-010-9381-1},
volume = {24},
year = {2010}
}
