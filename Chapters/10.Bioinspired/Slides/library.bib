pages = {715--770},
title = {{Slow feature analysis: unsupervised learning of invariances}},
url = {http://citeseerx.ist.psu.edu/viewdoc/download?doi=10.1.1.164.6721{\&}rep=rep1{\&}type=pdf},
volume = {14},
year = {2002}
}
