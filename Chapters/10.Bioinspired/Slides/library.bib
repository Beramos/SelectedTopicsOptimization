journal = {PLoS computational biology},
keywords = {Algorithms,Amino Acid Sequence,Area Under Curve,Artificial Intelligence,Binding Sites,Computer Simulation,Databases, Protein,Drug Discovery,Drug Discovery: methods,Feedback,Ligands,Models, Chemical,Models, Statistical,Protein Binding,Protein Interaction Domains and Motifs,Protein Interaction Mapping,Proteins,Proteins: chemistry,Proteins: metabolism,Receptors, Androgen,Receptors, Androgen: chemistry,Receptors, Androgen: metabolism,Reproducibility of Results,Sequence Analysis, Protein},
