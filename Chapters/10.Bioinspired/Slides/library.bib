number = {2},
pages = {0354--0362},
pmid = {17253905},
title = {{Linkage rules for plant-pollinator networks: Trait complementarity or exploitation barriers?}},
url = {http://journals.plos.org/plosbiology/article/file?id=10.1371/journal.pbio.0050031{\&}type=printable},
