doi = {10.1111/j.1365-2745.2007.01271.x},
file = {:Users/michielstock/Library/Application Support/Mendeley Desktop/Downloaded/Nielsen, Bascompte - 2007 - Ecological networks, nestedness and sampling effort.pdf:pdf},
