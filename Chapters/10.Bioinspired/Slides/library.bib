keywords = {Animals,Cell Dedifferentiation,Cell Dedifferentiation: physiology,Cell Differentiation,Cell Differentiation: physiology,Computer Simulation,Embryonic Stem Cells,Embryonic Stem Cells: physiology,Epigenesis, Genetic,Gene Regulatory Networks,Mice,Models, Genetic,Pluripotent Stem Cells,Pluripotent Stem Cells: physiology,Transcription Factors,Transcription Factors: genetics},
