keywords = {virus},
mendeley-tags = {virus},
pages = {201706951},
pmid = {29087310},
title = {{Rewriting nature's assembly manual for a ssRNA virus}},
url = {http://www.pnas.org/content/114/46/12255.full.pdf http://www.pnas.org/lookup/doi/10.1073/pnas.1706951114},
